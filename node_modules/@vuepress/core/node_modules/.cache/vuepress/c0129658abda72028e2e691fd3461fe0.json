ex) in projects\"\r\n            :key=\"index\"\r\n          >\r\n            <project-item :options=\"item\"></project-item>\r\n          </article>\r\n        </section>\r\n      </section>\r\n    </Common>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import Common from \"@theme/components/Common.vue\";\r\n  import { httpGet } from \"./utils/fetch.js\";\r\n  import ProjectItem from \"./ProjectItem\";\r\n  import { filter, orderBy } from \"lodash\";\r\n  export default {\r\n    name: \"Projects\",\r\n    components: {\r\n      Common,\r\n      ProjectItem\r\n    },\r\n    data() {\r\n      return {\r\n        projects: [],\r\n        loading: true\r\n      };\r\n    },\r\n    mounted() {\r\n      this.getProjects();\r\n    },\r\n    methods: {\r\n      getProjects() {\r\n        this.loading = true;\r\n        httpGet(\"https://api.github.com/users/LJoson/repos\").then(res => {\r\n          this.loading = false;\r\n          // 过滤掉私有的项目, 暂不过滤掉 fork 项目 && !item.fork\r\n          const projects = filter(res, item => !item.private);\r\n          this.projects = orderBy(\r\n            projects,\r\n            [\"stargazers_count\", \"forks_count\", \"name\", \"created_at\"],\r\n            [\"desc\", \"desc\", \"asc\", \"desc\"]\r\n          );\r\n        });\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n<style lang=\"css\" scoped>\r\n  .project {\r\n    max-width: 1024px;\r\n    min-height: 300px;\r\n    margin: 4rem auto;\r\n    margin-top: 6rem;\r\n  }\r\n  .project-title,\r\n  .project-subtitle {\r\n    text-align: center;\r\n  }\r\n  .project-loading {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  .project-loading img {\r\n    position: relative;\r\n  }\r\n  .project-loading span {\r\n    position: absolute;\r\n  }\r\n  .project-container {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-around;\r\n  }\r\n</style>"]}]}