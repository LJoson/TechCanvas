alStorage(\"todolist\",this.list);\r\n\t\t\t},\r\n\r\n\t\t\t// localStorage 方法\r\n\t\t\tgetLocalStorage(key) {\r\n\t\t\t\tvar todolist = JSON.parse(localStorage.getItem(key));\r\n\t\t\t\tthis.list = todolist;\r\n\t\t\t},\r\n\t\t\tsaveLocalStorage(key,val) {\r\n\t\t\t\tlocalStorage.setItem(key, JSON.stringify(val));\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n\t.todo {\r\n\t\twidth: 300px;\r\n\t\tbackground: #eee;\r\n\t\tmargin-left: 68%;\r\n\t}\r\n</style>"],"sourceRoot":"docs/.vuepress/components"}]}